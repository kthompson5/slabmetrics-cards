{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id","serial","player","set","images","weights","subgrades","graded_at"],
  "properties": {
    "id": { "type": "string" },
    "serial": { "type": "string" },
    "player": { "type": "string" },
    "set": { "type": "string" },
    "number": { "type": "string" },
    "variant": { "type": "string" },
    "images": {
      "type": "object",
      "required": ["front"],
      "properties": {
        "front": { "type": "string" },
        "back": { "type": ["string","null"] },
        "front_thumb": { "type": ["string","null"] },
        "back_thumb": { "type": ["string","null"] }
      }
    },
    "graded_at": { "type": "string" },
    "weights": {
      "type": "object",
      "required": ["front","back"],
      "properties": { "front": { "type": "number" }, "back": { "type": "number" } }
    },
    "subgrades": {
      "type": "object",
      "required": ["front","back"],
      "properties": {
        "front": { "type": "object" },
        "back": { "type": "object" }
      }
    },
    "grade_overall": { "type": ["number","null"] },
    "card_info": { "type": "object" },
    "links": { "type": "object" },
    "compare": { "type": "object" }
  }
}
